{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint-staged:eslint": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx",
    "lint-staged:style": "stylelint --fix --custom-syntax postcss-scss",
    "lint-staged:prettier": "prettier -c --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@next/font": "13.0.7",
    "@storybook/react": "^6.5.15",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "axios": "^1.2.1",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "next": "13.0.7",
    "postcss": "^8.4.20",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.57.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "*",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "storybook-addon-next": "^1.7.1",
    "stylelint": "^14.16.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,scss,md,json}": [
      "yarn lint-staged:prettier"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint-staged:eslint"
    ],
    "**/*.scss": [
      "yarn lint-staged:style"
    ]
  }
}
